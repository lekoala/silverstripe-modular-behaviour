{
  "name": "lekoala-silverstripe-modular-behaviour",
  "description": "Silverstripe Modular Behaviour integration",
  "scripts": {
    "copy": "shx cp -r ./node_modules/modular-behaviour.js/*.js ./client/ && shx cp -r ./node_modules/modular-behaviour.js/utils/*.js ./client/utils/",
    "build-iife": "esbuild --mangle-props=^_ --bundle --minify --format=iife --sourcemap client/modular-behaviour.js --outfile=client/modular-behaviour.min.js",
    "update-deps": "npx npm-check-updates -u"
  },
  "author": "LeKoala",
  "license": "MIT",
  "dependencies": {
    "modular-behaviour.js": "git://github.com/lekoala/modular-behaviour.js.git#1e18eab7d218ee404d526f092f7683812ded97a4"
  },
  "keywords": []
}
